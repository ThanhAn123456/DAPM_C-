@model IEnumerable<DAPM_C_.Models.DeXuat>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tieude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayDeXuat)
            </th>
            <th>
                Cửa hàng
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tieude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayDeXuat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaCuaHangNavigation.TenCuahang)
            </td>
            <td>
                <a href="@Url.Action("Details", new { id = item.MaDeXuat })">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
